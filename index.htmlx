<script>
(function(){
  if (localStorage.getItem('pp_cookie_consent')) return;
  document.addEventListener('DOMContentLoaded', function(){
    var style = document.getElementById('cookie-style');
    var div = document.createElement('div');
    div.innerHTML = `\n<div id="cookie-banner" aria-live="polite" style="display:none">\n  <div style="flex:1">\n    <p><strong>We use cookies</strong> to improve your experience. Manage preferences in Privacy Policy.</p>\n  </div>\n  <div>\n    <button class="cookie-btn" id="accept-cookies">Accept</button>\n    <button class="cookie-btn secondary" id="manage-cookies">Manage</button>\n  </div>\n</div>\n`;
    document.body.appendChild(div);
    var banner = document.getElementById('cookie-banner');
    banner.style.display = 'flex';
    document.getElementById('accept-cookies').addEventListener('click', function(){
      localStorage.setItem('pp_cookie_consent','accepted');
      banner.style.display='none';
    });
    document.getElementById('manage-cookies').addEventListener('click', function(){
      window.location.href = '/privacy.html';
    });
  });
})();
</script>

